

<%= form_for @address do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :street %>
<%= f.text_field :street %>
<%= f.label :city %>
<%= f.text_field :city %>
<%= f.label :state %>
<%= f.text_field :state %>
<%= f.label :zipcode %>
<%= f.text_field :zipcode %>
<%= f.submit "Save Address" %>
<% end %>